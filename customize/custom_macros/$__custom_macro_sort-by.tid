\define belongs-sort-state(title) $:/state/sort/$(currentTiddler)$/$title$/$(sbb)$

\define belongs-display(filter, cfilter, title, checkbox, sort-order)
<$reveal default="$cfilter$" type="match" text="" tag="div" class="belongs-display"><$transclude $variable="record" filter=<<__filter__>>/></$reveal>
<$reveal default="$cfilter$" type="nomatch" text="" tag="div"><$transclude $variable="table" rfilter=<<__filter__>> cfilter=<<__cfilter__>> title=<<__title__>> checkbox=<<__checkbox__>> sort-order=<<__sort-order__>>/></$reveal>
\end

